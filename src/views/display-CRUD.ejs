<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Document</title>
</head>

<body>
    <table>
        <tr>
            <th>email</th>
            <th>first name</th>
            <th>last name</th>
            <th>address</th>
            <th>edit</th>
        </tr>
        <% for(var i=0; i < dataTable.length; i++) { %>
            <tr>
                <td>
                    <%= dataTable[i].email %>
                </td>
                <td>
                    <%= dataTable[i].firstName %>
                </td>
                <td>
                    <%= dataTable[i].lastName %>
                </td>
                <td>
                    <%= dataTable[i].address %>
                </td>
                <td>
                    <a href="/edit-crud?id=<%= dataTable[i].id%>" class="btn-edit" type="button">edit</a>
                    <a href="/delete-crud?id=<%= dataTable[i].id%>" class="btn-delete" type="button">delete</a>
                </td>
            </tr>
            <% } %>
    </table>
</body>

</html>